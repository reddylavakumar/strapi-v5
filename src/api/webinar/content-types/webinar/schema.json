      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "EventType": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::event-type.event-type"
    },
    "EventFormat": {
      "type": "relation",
      "relation": "oneToMany",
